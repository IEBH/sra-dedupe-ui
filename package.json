{
  "name": "sra-dedupe-ui",
  "version": "0.0.0",
  "description": "Front-end UI for the sra-dedupe module",
  "main": "app.js",
  "scripts": {
    "build": "electron-builder -mwl",
    "build-lin": "electron-builder --linux --x64",
    "build-win": "electron-builder --win --x64",
    "build-win32": "electron-builder --win --ia32",
    "build-darwin": "electron-builder --mac"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CREBP/sra-dedupe-ui.git"
  },
  "keywords": [
    "crebp",
    "sra",
    "dedupe",
    "deduplicate",
    "medical",
    "research"
  ],
  "author": "Matt Carter <m@ttcarter.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CREBP/sra-dedupe-ui/issues"
  },
  "homepage": "https://github.com/CREBP/sra-dedupe-ui#readme",
  "dependencies": {
    "angular": "^1.6.5",
    "async-chainable": "^2.3.2",
    "base64url": "^2.0.0",
    "bootstrap": "^3.3.7",
    "chalk": "^2.0.1",
    "commander": "^2.11.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "reflib": "^1.5.6",
    "sra-dedupe": "^1.0.6",
    "stream-chunker": "^1.2.8",
    "string-to-stream": "^1.1.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-angularjs-annotate": "^0.7.0",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "electron-builder": "^19.12.0",
    "electron-prebuilt": "^1.4.13",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-watch": "^4.3.11",
    "image-webpack-loader": "^3.3.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack-stream": "^3.2.0"
  },
  "engines": {
    "node": "6.0.0"
  },
  "build": {
    "appId": "com.crebp-sra",
    "productName": "SRA-DeDupe",
    "artifactName": "sra-dedupe-${os}-${arch}.${ext}",
    "copyright": "Copyright Â© 2017 Bond University Centre for Research in Evidence-Based Practice",
    "files": [
      "app.js",
      "build/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "linux": {
      "packageCategory": "education",
      "icon": "app/img/icon",
      "target": [
        "AppImage",
	"deb",
	"rpm",
	"tar.bz2"
      ]
    },
    "mac": {
      "category": "public.app-category.education",
      "icon": "app/img/icon/256x256.icns"
    },
    "nsis": {
      "artifactName": "sra-dedupe-${os}-${arch}-installer.${ext}",
      "license": "./LICENSE"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "app/img/icon/256x256.ico"
    }
  }
}
